   "source": [
    "import cv2\n",
    "\n",
    "for i in range(10):  # 0~4까지 디바이스 인덱스를 확인\n",
    "    cap = cv2.VideoCapture(i)\n",
    "    if cap.isOpened():\n",
    "        print(f\"Camera index {i} is available.\")\n",
    "        cap.release()\n",
    "    else:\n",
    "        print(f\"Camera index {i} is not available.\")\n",
    "\n"
   ]
